{
    "name": "SyntaxHighlight_PrismJS",
    "author": ["apple502j"],
    "descriptionmsg": "syntaxhighlight-desc",
    "license-name": "GPL-3.0-or-later",
    "type": "parserhook",
    "version": "1.0.0",
    "url": "https://github.com/InternationalScratchWiki/SyntaxHighlight_PrismJS",
    "AutoloadClasses": {
        "SyntaxHighlight": "includes/SyntaxHighlight.php"
    },
    "Hooks": {
        "ParserFirstCallInit": "main",
        "EditPage::showEditForm:initial": "main",
        "GetPreferences": "main",
        "ParserOptionsRegister": "main"
    },
    "HookHandlers": {
        "main": {
            "class": "SyntaxHighlight",
            "services": ["UserOptionsLookup"]
        }
    },
    "MessagesDirs": {
        "SyntaxHighlight_PrismJS": ["i18n"]
    },
    "DefaultUserOptions": {
        "syntaxhighlight-theme": "default"
    },
    "manifest_version": 2
}
